{
  "name": "service",
  "version": "2024.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "bundle:encrypt": "rollup -c scripts/encrypt.ts --configPlugin esbuild",
    "bundle:parser": "rollup -c scripts/parser.ts --configPlugin esbuild",
    "debug": "cross-env NODE_ENV=development pnpm build && cross-env NODE_ENV=development node dist/index.js --expose-gc",
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "lint": "prettier --check --write . && eslint . --ext .ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts",
    "run": "npm upgrade && npm run build && node dist/index.js",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "packageManager": "pnpm@8.12.0",
  "nano-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,json,yaml,yml,md}": "prettier --write"
  },
  "dependencies": {
    "@alicloud/alinlp20200629": "2.0.2",
    "@alicloud/openapi-client": "0.4.7",
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.6",
    "@types/crypto-js": "4.2.1",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "20.10.4",
    "@types/qrcode": "1.5.5",
    "@types/set-cookie-parser": "2.4.7",
    "body-parser": "1.20.2",
    "cheerio": "1.0.0-rc.12",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cross-env": "7.0.3",
    "crypto-js": "4.2.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "iconv-lite": "0.6.3",
    "morgan": "1.10.0",
    "qrcode": "1.5.3",
    "set-cookie-parser": "2.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "cross-env": "7.0.3",
    "esbuild": "0.19.9",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "prettier": "3.1.1",
    "rollup": "3.29.4",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-esbuild": "6.1.0",
    "tsx": "4.6.2",
    "typescript": "5.3.3"
  }
}
