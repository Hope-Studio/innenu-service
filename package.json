    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.6",
    "@types/crypto-js": "4.2.2",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "20.11.19",
    "@types/qrcode": "1.5.5",
    "body-parser": "1.20.2",
    "cheerio": "1.0.0-rc.12",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cross-env": "7.0.3",
