{
  "name": "service",
  "version": "2024.8.12",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "bundle:encrypt": "rollup -c scripts/encrypt.ts --configPlugin esbuild",
    "debug": "cross-env NODE_ENV=development pnpm build && cross-env NODE_ENV=development node dist/index.js --expose-gc",
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "lint": "eslint --fix . && prettier --check --write .",
    "lint:check": "eslint . && prettier --check .",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@alicloud/alinlp20200629": "3.1.0",
    "@alicloud/openapi-client": "0.4.11",
    "@mptool/net": "0.9.0",
    "@mptool/parser": "0.9.0",
    "body-parser": "1.20.2",
    "cheerio": "1.0.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "crypto-js": "4.2.0",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "iconv-lite": "0.6.3",
    "js-sha1": "0.7.0",
    "morgan": "1.10.0",
    "mysql2": "3.11.0",
    "picocolors": "1.0.1",
    "qrcode": "1.5.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@rollup/plugin-commonjs": "26.0.1",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/crypto-js": "4.2.2",
    "@types/express": "4.17.21",
    "@types/express-serve-static-core": "4.19.5",
    "@types/morgan": "1.9.9",
    "@types/node": "22.3.0",
    "@types/qrcode": "1.5.5",
    "@types/uuid": "10.0.0",
    "cross-env": "7.0.3",
    "cz-git": "1.9.4",
    "esbuild": "0.23.0",
    "eslint": "9.9.0",
    "eslint-config-mister-hope": "0.2.3",
    "express-rate-limit": "7.4.0",
    "husky": "9.1.4",
    "nano-staged": "0.8.0",
    "prettier": "3.3.3",
    "rollup": "4.20.0",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-esbuild": "6.1.1",
    "tsx": "4.17.0",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": ">= 20"
  }
}
