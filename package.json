{
  "name": "service",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "debug": "cross-env http_proxy=http://127.0.0.1:8888 https_proxy=http://127.0.0.1:8888 NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "lint": "prettier --check --write . && eslint . --ext .ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts",
    "run": "npm i && npm run build && node dist/index.js"
  },
  "packageManager": "pnpm@8.6.3",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "iconv-lite": "0.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "cross-env": "7.0.3",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "^2.8.8",
    "rollup": "^3.25.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  }
}
