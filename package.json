    "lint": "prettier --check --write . && eslint . --ext .ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts",
    "prepare": "husky"
  },
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint --fix"
  },
